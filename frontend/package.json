{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --watch",
    "testing": "jest",
    "test:ci": "jest --ci",
    "coverage": "jest --coverage",
    "custom-pre-commit": "echo 'Running custom pre-commit checks for frontend'",
    "lint": "next lint && tsc --noEmit",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "npm run check-lint --fix && npm run testing && npx prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "framer-motion": "^10.16.4",
    "jest-environment-jsdom": "^29.7.0",
    "next": "13.5.6",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "autoprefixer": "^10",
    "eslint": "^8.52.0",
    "eslint-config-next": "^13.5.6",
    "eslint-plugin-testing-library": "^6.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "postcss": "^8",
    "prettier": "^3.0.3",
    "tailwindcss": "^3",
    "typescript": "^5"
  }
}
